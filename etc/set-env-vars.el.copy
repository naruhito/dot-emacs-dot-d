;;; set-env-vars.el --- Set environment variables
(if (not (getenv "USERNAME"))
    (setenv "USERNAME" "naruhito"))

(if (not (getenv "USER_FULLNAME"))
    (setenv "USER_FULLNAME" "Naruhito Nakaharai"))

(if (not (getenv "HOSTNAME"))
    (setenv "HOSTNAME" "localhost"))

(defvar gui-frame-settings
  '((top . 0) (left . 0)
    (height . 35) (width . 120)))

(provide 'set-env-vars)
