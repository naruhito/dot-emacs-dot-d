[user]
	name = Naruhito
	email = naruhito78@gmail.com
[alias]
	br = branch
	st = status
	ci = commit
	co = checkout
	di = diff
	lg = log -5 --name-status
	oneline = log --pretty=format:"%h%x09%an%x09%ad%x09%s"
	unstage = reset HEAD
	last = log -1 -p HEAD
	amend = commit --amend
	discard = checkout
	graph = log --graph --decorate
[color]
	ui = auto
[core]
	pager = less
	excludesfile = ~/.gitignore
[push]
	default = simple
